{
    "name": "flashcards-back",
    "version": "1.0.0",
    "description": "Simple Flashcards Software - Backend",
    "main": "index.js",
    "scripts": {
        "start": "NODE_ENV=development node src/index.js",
        "start-clustered": "NODE_ENV=development APP_CLUSTERED=true node src/index.js",
        "test": "NODE_ENV=test mocha src/**/*.test.js",
        "test-cover": "istanbul cover _mocha $(find ./src -name \"*.test.js\" -not -path \"./node_modules/*\")"
    },
    "author": "Dione Betanin",
    "license": "MIT",
    "dependencies": {
        "bcrypt": "1.0.3",
        "body-parser": "1.18.2",
        "cluster": "0.7.7",
        "express": "4.16.2",
        "express-session": "1.15.6",
        "jsonwebtoken": "8.2.0",
        "mongoose": "5.0.4",
        "morgan": "1.9.0",
        "os": "0.1.1",
        "passport": "0.4.0",
        "passport-local": "1.0.0",
        "path": "0.12.7",
        "rotating-file-stream": "1.3.4",
        "validator": "9.4.1"
    },
    "devDependencies": {
        "chai": "4.1.2",
        "istanbul": "0.4.5",
        "mocha": "5.0.0",
        "sinon": "4.4.8"
    }
}
